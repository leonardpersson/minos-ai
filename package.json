{
    "name": "minos-ai",
    "version": "0.1.0",
    "private": true,
    "description": "Advanced AI-Powered DeFi Strategy Platform on Solana",
    "author": "Minos-AI Foundation <dev@minos-ai.org>",
    "license": "MIT",
    "homepage": "https://minos-ai.org",
    "workspaces": [
      "packages/*",
      "sdk"
    ],
    "repository": {
      "type": "git",
      "url": "https://github.com/minos-ai/minos-ai.git"
    },
    "engines": {
      "node": ">=18",
      "npm": ">=9"
    },
    "scripts": {
      "prepare": "husky install",
      "build": "lerna run build --stream",
      "clean": "lerna run clean --stream && rm -rf node_modules",
      "lint": "lerna run lint --stream",
      "lint:fix": "lerna run lint:fix --stream",
      "test": "lerna run test --stream",
      "test:coverage": "lerna run test:coverage --stream",
      "dev": "lerna run dev --stream --parallel",
      "indexer:dev": "lerna run dev --scope=@minos-ai/indexer --stream",
      "backend:dev": "lerna run dev --scope=@minos-ai/backend --stream",
      "deploy:devnet": "cd packages/contracts && npm run deploy:devnet",
      "deploy:mainnet": "cd packages/contracts && npm run deploy:mainnet",
      "init:devnet": "cd packages/contracts && npm run init:devnet",
      "docs:generate": "typedoc --options typedoc.json",
      "docs:dev": "docsify serve ./docs",
      "format": "prettier --write \"**/*.{ts,js,md,json}\"",
      "release": "lerna version --conventional-commits --yes",
      "publish:packages": "lerna publish from-git --yes",
      "precommit": "lint-staged",
      "check:deps": "npm-check-updates -u",
      "docker:dev": "docker-compose -f docker/dev/docker-compose.yml up -d",
      "docker:prod": "docker-compose -f docker/prod/docker-compose.yml up -d",
      "docker:down": "docker-compose -f docker/dev/docker-compose.yml down",
      "validate": "npm run lint && npm run test && npm run build"
    },
    "devDependencies": {
      "@commitlint/cli": "^17.6.5",
      "@commitlint/config-conventional": "^17.6.5",
      "@types/jest": "^29.5.2",
      "@types/node": "^18.16.16",
      "@typescript-eslint/eslint-plugin": "^5.59.9",
      "@typescript-eslint/parser": "^5.59.9",
      "docsify-cli": "^4.4.4",
      "eslint": "^8.42.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-jest": "^27.2.1",
      "eslint-plugin-prettier": "^4.2.1",
      "husky": "^8.0.3",
      "jest": "^29.5.0",
      "lerna": "^6.6.2",
      "lint-staged": "^13.2.2",
      "npm-check-updates": "^16.10.12",
      "prettier": "^2.8.8",
      "rimraf": "^5.0.1",
      "ts-jest": "^29.1.0",
      "ts-node": "^10.9.1",
      "typedoc": "^0.24.8",
      "typescript": "^5.1.3"
    },
    "lint-staged": {
      "*.{ts,js}": [
        "eslint --fix",
        "prettier --write"
      ],
      "*.{json,md}": [
        "prettier --write"
      ]
    },
    "commitlint": {
      "extends": [
        "@commitlint/config-conventional"
      ]
    },
    "keywords": [
      "blockchain",
      "solana",
      "defi",
      "artificial-intelligence",
      "machine-learning",
      "trading",
      "prediction",
      "vault",
      "finance",
      "crypto",
      "decentralized",
      "governance"
    ],
    "bugs": {
      "url": "https://github.com/minos-ai/minos-ai/issues"
    },
    "contributors": [
      {
        "name": "Theseus Daedalus",
        "email": "theseus@minos-ai.org"
      },
      {
        "name": "Ariadne Minos",
        "email": "ariadne@minos-ai.org"
      },
      {
        "name": "Perseus Androgeus",
        "email": "perseus@minos-ai.org"
      }
    ],
    "funding": {
      "type": "foundation",
      "url": "https://minos-ai.org/funding"
    }
  }