{
    "name": "@minos-ai/backend",
    "version": "0.1.0",
    "description": "Enterprise-grade backend service for the Minos-AI DeFi strategy platform on Solana",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "private": true,
    "engines": {
      "node": ">=18.0.0",
      "npm": ">=8.0.0"
    },
    "scripts": {
      "build": "rimraf dist && tsc -p tsconfig.json",
      "build:watch": "tsc -p tsconfig.json --watch",
      "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/index.ts",
      "start": "cross-env NODE_ENV=production node dist/index.js",
      "start:dev": "cross-env NODE_ENV=development nodemon dist/index.js",
      "start:staging": "cross-env NODE_ENV=staging node dist/index.js",
      "lint": "eslint \"src/**/*.ts\" --fix",
      "format": "prettier --write \"src/**/*.ts\"",
      "test": "jest --config=jest.config.js",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "test:e2e": "jest --config=jest.e2e.config.js",
      "test:unit": "jest --config=jest.unit.config.js",
      "test:integration": "jest --config=jest.integration.config.js",
      "clean": "rimraf dist coverage .nyc_output",
      "check-types": "tsc --noEmit",
      "prepare": "husky install",
      "docs": "typedoc --out docs src",
      "audit": "npm audit --production",
      "audit:fix": "npm audit fix --production",
      "precommit": "lint-staged",
      "ci": "npm run lint && npm run test && npm run build",
      "db:migrate": "ts-node ./scripts/migrate.ts",
      "db:seed": "ts-node ./scripts/seed.ts",
      "analyze": "source-map-explorer 'dist/**/*.js'",
      "docker:build": "docker build -t minos-ai/backend:latest .",
      "docker:run": "docker run -p 3000:3000 minos-ai/backend:latest",
      "deploy:staging": "bash ./scripts/deploy-staging.sh",
      "deploy:production": "bash ./scripts/deploy-production.sh",
      "monitor": "cross-env NODE_ENV=production pm2 start dist/index.js --name minos-ai-backend",
      "generate:keypair": "ts-node ./scripts/generate-keypair.ts",
      "validate:config": "ts-node ./scripts/validate-config.ts"
    },
    "dependencies": {
      "@metaplex-foundation/js": "^0.19.4",
      "@metaplex/js": "^4.12.0",
      "@project-serum/anchor": "^0.26.0",
      "@sentry/node": "^7.68.0",
      "@solana/spl-token": "^0.3.8",
      "@solana/web3.js": "^1.78.0",
      "ajv": "^8.12.0",
      "axios": "^1.5.1",
      "body-parser": "^1.20.2",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "dayjs": "^1.11.10",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-async-errors": "^3.1.1",
      "express-rate-limit": "^7.1.0",
      "express-validator": "^7.0.1",
      "helmet": "^7.0.0",
      "ioredis": "^5.3.2",
      "joi": "^17.11.0",
      "jsonwebtoken": "^9.0.2",
      "lodash": "^4.17.21",
      "mongoose": "^7.5.3",
      "morgan": "^1.10.0",
      "multer": "^1.4.5-lts.1",
      "newrelic": "^11.0.0",
      "node-cache": "^5.1.2",
      "node-cron": "^3.0.2",
      "node-fetch": "^3.3.2",
      "pino": "^8.16.0",
      "pino-http": "^8.5.0",
      "pino-pretty": "^10.2.3",
      "prom-client": "^14.2.0",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.0",
      "typeorm": "^0.3.17",
      "uuid": "^9.0.1",
      "winston": "^3.11.0",
      "winston-daily-rotate-file": "^4.7.1",
      "ws": "^8.14.2",
      "zod": "^3.22.4"
    },
    "devDependencies": {
      "@commitlint/cli": "^17.7.2",
      "@commitlint/config-conventional": "^17.7.0",
      "@types/compression": "^1.7.3",
      "@types/cors": "^2.8.14",
      "@types/express": "^4.17.19",
      "@types/jest": "^29.5.5",
      "@types/jsonwebtoken": "^9.0.3",
      "@types/lodash": "^4.14.199",
      "@types/morgan": "^1.9.6",
      "@types/multer": "^1.4.8",
      "@types/node": "^20.8.5",
      "@types/node-cron": "^3.0.9",
      "@types/supertest": "^2.0.14",
      "@types/swagger-jsdoc": "^6.0.1",
      "@types/swagger-ui-express": "^4.1.4",
      "@types/uuid": "^9.0.5",
      "@types/ws": "^8.5.6",
      "@typescript-eslint/eslint-plugin": "^6.7.4",
      "@typescript-eslint/parser": "^6.7.4",
      "cross-env": "^7.0.3",
      "eslint": "^8.51.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-import": "^2.28.1",
      "eslint-plugin-jest": "^27.4.2",
      "eslint-plugin-prettier": "^5.0.0",
      "eslint-plugin-security": "^1.7.1",
      "husky": "^8.0.3",
      "jest": "^29.7.0",
      "jest-extended": "^4.0.1",
      "jest-mock-extended": "^3.0.5",
      "lint-staged": "^14.0.1",
      "mockdate": "^3.0.5",
      "nock": "^13.3.3",
      "nodemon": "^3.0.1",
      "pm2": "^5.3.0",
      "prettier": "^3.0.3",
      "rimraf": "^5.0.5",
      "source-map-explorer": "^2.5.3",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.1",
      "ts-node-dev": "^2.0.0",
      "typedoc": "^0.25.2",
      "typescript": "^5.2.2"
    },
    "lint-staged": {
      "*.ts": [
        "eslint --fix",
        "prettier --write"
      ]
    },
    "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "coveragePathIgnorePatterns": [
        "/node_modules/",
        "/dist/"
      ]
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/minos-ai/defi-platform.git",
      "directory": "packages/backend"
    },
    "bugs": {
      "url": "https://github.com/minos-ai/defi-platform/issues"
    },
    "homepage": "https://minos-ai.io",
    "author": {
      "name": "Minos-AI Team",
      "email": "dev@minos-ai.io",
      "url": "https://minos-ai.io"
    },
    "keywords": [
      "defi",
      "blockchain",
      "solana",
      "ai",
      "trading",
      "finance",
      "crypto",
      "investment",
      "strategy",
      "vault",
      "web3"
    ],
    "publishConfig": {
      "access": "restricted"
    },
    "browserslist": [
      "node >= 18"
    ],
    "volta": {
      "node": "18.17.1",
      "npm": "9.8.1"
    }
  }