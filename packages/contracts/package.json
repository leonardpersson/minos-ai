{
    "name": "@minos/contracts",
    "version": "0.1.0",
    "description": "Smart contracts for Minos-AI DeFi strategy platform on Solana blockchain",
    "private": true,
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
      "dist",
      "idl",
      "target/idl",
      "target/types",
      "src",
      "README.md"
    ],
    "scripts": {
      "build": "anchor build",
      "build:debug": "anchor build -- --features debug-mode",
      "build:local": "anchor build -- --features local-testing",
      "build:devnet": "anchor build -- --features devnet-testing",
      "build:mainnet": "anchor build -- --features mainnet",
      "build:ts": "tsc -p tsconfig.json",
      "deploy": "anchor deploy",
      "deploy:devnet": "anchor deploy --provider.cluster devnet",
      "deploy:mainnet": "anchor deploy --provider.cluster mainnet-beta",
      "deploy:upgradeable": "ts-node scripts/deploy-upgradeable.ts",
      "test": "anchor test",
      "test:local": "anchor test -- --features local-testing",
      "test:devnet": "anchor test -- --provider.cluster devnet",
      "test:unit": "yarn run ts-mocha -p ./tsconfig.json -t 1000000 tests/unit/**/*.ts",
      "test:integration": "yarn run ts-mocha -p ./tsconfig.json -t 1000000 tests/integration/**/*.ts",
      "test:coverage": "nyc --reporter=lcov --reporter=text anchor test",
      "benchmark": "ts-node tests/benchmark/run-benchmarks.ts",
      "clean": "rm -rf .anchor target dist node_modules .nyc_output coverage",
      "clean:build": "rm -rf .anchor target dist",
      "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
      "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
      "lint:rust": "cargo clippy -- -D warnings",
      "lint:rust:fix": "cargo clippy --fix -- -D warnings",
      "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
      "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
      "format:rust": "cargo fmt --all -- --check",
      "format:rust:write": "cargo fmt --all",
      "docs": "typedoc --entryPointStrategy expand ./tests && cargo doc --no-deps --open",
      "docs:ts": "typedoc --entryPointStrategy expand ./tests",
      "docs:rust": "cargo doc --no-deps --open",
      "idl:generate": "yarn run ts-node scripts/parse-idl.ts",
      "idl:update": "yarn run ts-node scripts/update-idl.ts",
      "analyze": "anchor analyze",
      "analyze:security": "cargo-audit audit",
      "analyze:dependencies": "npm audit && cargo audit",
      "verify": "yarn lint && yarn test && yarn analyze:security",
      "start-validator": "solana-test-validator --reset --quiet",
      "start-validator:clone": "solana-test-validator --reset --quiet --bpf-program TokenkegQfeZyiNwAJbNbGKPFXCWuBvf9Ss623VQ5DA target/deploy/spl_token.so --bpf-program ATokenGPvbdGVxr1b2hvZbsiqW5xWH25efTNsLJA8knL target/deploy/spl_associated_token_account.so --bpf-program metaqbxxUerdq28cj1RbAWkYQm3ybzjb6a8bt518x1s target/deploy/mpl_token_metadata.so",
      "migrate": "ts-node scripts/migrations/run-migration.ts",
      "migrate:devnet": "ts-node scripts/migrations/run-migration.ts --cluster devnet",
      "airdrop": "ts-node scripts/utils/airdrop.ts",
      "airdrop:devnet": "ts-node scripts/utils/airdrop.ts --cluster devnet",
      "update-program-id": "ts-node scripts/update-program-id.ts",
      "initialize-vault": "ts-node scripts/initialize-vault.ts",
      "create-test-accounts": "ts-node scripts/create-test-accounts.ts",
      "monitor": "ts-node scripts/monitor-transactions.ts",
      "calculate-fees": "ts-node scripts/utils/calculate-fees.ts",
      "simulate": "ts-node scripts/simulate-strategy.ts",
      "simulate:risk": "ts-node scripts/simulate-risk-scenarios.ts",
      "prepare": "husky install",
      "postinstall": "ts-node scripts/verify-environment.ts",
      "precommit": "lint-staged && cargo clippy -- -D warnings",
      "prepublishOnly": "yarn clean && yarn build && yarn test && yarn lint",
      "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
    },
    "dependencies": {
      "@coral-xyz/anchor": "^0.29.0",
      "@metaplex-foundation/mpl-token-metadata": "^2.13.0",
      "@project-serum/anchor": "^0.26.0",
      "@project-serum/serum": "^0.13.65",
      "@pythnetwork/client": "^2.19.0",
      "@solana/buffer-layout": "^4.0.1",
      "@solana/buffer-layout-utils": "^0.2.0",
      "@solana/spl-token": "^0.3.8",
      "@solana/spl-token-registry": "^0.2.7",
      "@solana/web3.js": "^1.78.5",
      "bn.js": "^5.2.1",
      "borsh": "^1.0.0",
      "bs58": "^5.0.0",
      "buffer-layout": "^1.2.2",
      "decimal.js": "^10.4.3",
      "dotenv": "^16.3.1",
      "eventemitter3": "^5.0.1",
      "lodash": "^4.17.21"
    },
    "devDependencies": {
      "@commitlint/cli": "^17.7.1",
      "@commitlint/config-conventional": "^17.7.0",
      "@metaplex-foundation/js": "^0.19.4",
      "@rushstack/eslint-patch": "^1.5.1",
      "@solana/spl-governance": "^0.3.18",
      "@types/bn.js": "^5.1.3",
      "@types/chai": "^4.3.6",
      "@types/chai-as-promised": "^7.1.6",
      "@types/lodash": "^4.14.198",
      "@types/mocha": "^10.0.1",
      "@types/node": "^20.6.0",
      "@types/sinon": "^10.0.16",
      "@typescript-eslint/eslint-plugin": "^6.7.0",
      "@typescript-eslint/parser": "^6.7.0",
      "chai": "^4.3.8",
      "chai-as-promised": "^7.1.1",
      "chai-bn": "^0.3.1",
      "chalk": "^5.3.0",
      "conventional-changelog-cli": "^4.1.0",
      "eslint": "^8.49.0",
      "eslint-config-airbnb-typescript": "^17.1.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-import": "^2.28.1",
      "eslint-plugin-jest": "^27.2.3",
      "eslint-plugin-prettier": "^5.0.0",
      "esm": "^3.2.25",
      "husky": "^8.0.3",
      "jest": "^29.7.0",
      "jest-environment-node": "^29.7.0",
      "lint-staged": "^14.0.1",
      "mocha": "^10.2.0",
      "nyc": "^15.1.0",
      "prettier": "^3.0.3",
      "sinon": "^16.0.0",
      "ts-jest": "^29.1.1",
      "ts-mocha": "^10.0.0",
      "ts-node": "^10.9.1",
      "typedoc": "^0.25.1",
      "typescript": "^5.2.2"
    },
    "engines": {
      "node": ">=18.0.0",
      "npm": ">=9.0.0"
    },
    "lint-staged": {
      "*.{js,ts,jsx,tsx}": [
        "eslint --fix",
        "prettier --write"
      ],
      "*.json": [
        "prettier --write"
      ],
      "*.rs": [
        "cargo clippy --fix -- -D warnings",
        "cargo fmt"
      ]
    },
    "resolutions": {
      "@solana/web3.js": "^1.78.5",
      "bn.js": "^5.2.1",
      "json5": "^2.2.3",
      "minimist": "^1.2.8"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/minos-ai/minos-platform.git",
      "directory": "packages/contracts"
    },
    "bugs": {
      "url": "https://github.com/minos-ai/minos-platform/issues"
    },
    "homepage": "https://minos.ai",
    "keywords": [
      "solana",
      "anchor",
      "blockchain",
      "defi",
      "crypto",
      "ai",
      "machine-learning",
      "vault",
      "strategy",
      "yield",
      "smart-contracts",
      "prediction",
      "algorithmic",
      "trading",
      "portfolio",
      "optimization",
      "risk-management",
      "on-chain"
    ],
    "publishConfig": {
      "access": "restricted",
      "registry": "https://registry.npmjs.org/"
    },
    "commitlint": {
      "extends": [
        "@commitlint/config-conventional"
      ]
    },
    "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "collectCoverageFrom": [
        "src/**/*.{ts,tsx}",
        "!src/**/*.d.ts"
      ]
    }
  }