{
    "name": "@minos-ai/indexer",
    "version": "0.1.0",
    "description": "Blockchain indexer for the Minos-AI platform",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "private": true,
    "scripts": {
      "build": "tsc -p tsconfig.json",
      "clean": "rimraf dist",
      "start": "node dist/index.js",
      "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
      "lint": "eslint src --ext .ts",
      "test": "jest",
      "migrate:up": "node dist/scripts/migrate.js up",
      "migrate:down": "node dist/scripts/migrate.js down",
      "typeorm": "typeorm-ts-node-commonjs",
      "postinstall": "npm run build"
    },
    "keywords": [
      "blockchain",
      "indexer",
      "solana",
      "ai",
      "trading"
    ],
    "author": "Minos-AI Team <dev@minos-ai.com>",
    "license": "UNLICENSED",
    "dependencies": {
      "@project-serum/anchor": "^0.26.0",
      "@solana/web3.js": "^1.73.3",
      "bn.js": "^5.2.1",
      "bs58": "^5.0.0",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "joi": "^17.9.0",
      "pg": "^8.10.0",
      "pino": "^8.11.0",
      "pino-pretty": "^10.0.0",
      "reflect-metadata": "^0.1.13",
      "superstruct": "^1.0.3",
      "typeorm": "^0.3.12",
      "typescript": "^4.9.5",
      "uuid": "^9.0.0"
    },
    "devDependencies": {
      "@types/bn.js": "^5.1.1",
      "@types/express": "^4.17.17",
      "@types/jest": "^29.5.0",
      "@types/node": "^18.15.3",
      "@types/pg": "^8.6.6",
      "@types/uuid": "^9.0.1",
      "@typescript-eslint/eslint-plugin": "^5.55.0",
      "@typescript-eslint/parser": "^5.55.0",
      "eslint": "^8.36.0",
      "eslint-config-prettier": "^8.7.0",
      "eslint-plugin-prettier": "^4.2.1",
      "jest": "^29.5.0",
      "prettier": "^2.8.4",
      "rimraf": "^4.4.0",
      "ts-jest": "^29.0.5",
      "ts-node": "^10.9.1",
      "ts-node-dev": "^2.0.0"
    },
    "engines": {
      "node": ">=16.0.0"
    }
  }