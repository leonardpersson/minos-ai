{
    "name": "@minos-ai/sdk",
    "version": "0.2.0",
    "description": "Enterprise-grade TypeScript SDK for interacting with Minos AI DeFi platform on Solana blockchain",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
      "dist",
      "src",
      "README.md",
      "LICENSE"
    ],
    "sideEffects": false,
    "publishConfig": {
      "access": "public",
      "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
      "build": "rollup -c",
      "build:types": "tsc --emitDeclarationOnly",
      "build:full": "npm run clean && npm run build && npm run build:types",
      "clean": "rimraf dist coverage .nyc_output",
      "test": "jest --coverage",
      "test:watch": "jest --watch",
      "test:ci": "jest --ci --coverage",
      "lint": "eslint 'src/**/*.{ts,tsx}'",
      "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
      "format": "prettier --write 'src/**/*.{ts,tsx}'",
      "format:check": "prettier --check 'src/**/*.{ts,tsx}'",
      "docs": "typedoc --out docs src",
      "prepublishOnly": "npm run build:full",
      "preversion": "npm run lint && npm test",
      "version": "npm run format && git add -A src",
      "postversion": "git push && git push --tags",
      "examples": "ts-node examples/index.ts",
      "benchmark": "ts-node benchmark/index.ts",
      "analyze": "source-map-explorer dist/index.js",
      "release": "standard-version"
    },
    "keywords": [
      "minos-ai",
      "defi",
      "solana",
      "blockchain",
      "trading",
      "algorithm",
      "ai",
      "machine-learning",
      "sdk",
      "typescript",
      "crypto",
      "cryptocurrency",
      "web3",
      "vault",
      "strategy",
      "investment"
    ],
    "author": {
      "name": "Minos AI Team",
      "email": "dev@minos-ai.io",
      "url": "https://minos-ai.io"
    },
    "contributors": [
      {
        "name": "Minos AI Core Team",
        "email": "core@minos-ai.io"
      }
    ],
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/minos-ai/minos-ai.git",
      "directory": "sdk"
    },
    "bugs": {
      "url": "https://github.com/minos-ai/minos-ai/issues",
      "email": "bugs@minos-ai.io"
    },
    "homepage": "https://docs.minos-ai.io/sdk",
    "engines": {
      "node": ">=18.0.0"
    },
    "dependencies": {
      "@project-serum/anchor": "^0.26.0",
      "@solana/spl-token": "^0.3.8",
      "@solana/web3.js": "^1.78.5",
      "axios": "^1.6.0",
      "bn.js": "^5.2.1",
      "bs58": "^5.0.0",
      "buffer": "^6.0.3",
      "eventemitter3": "^5.0.1",
      "isomorphic-ws": "^5.0.0",
      "jayson": "^4.1.0",
      "lodash": "^4.17.21",
      "superstruct": "^1.0.3",
      "ws": "^8.14.2",
      "tweetnacl": "^1.0.3",
      "retry": "^0.13.1",
      "p-retry": "^6.0.0",
      "p-timeout": "^6.1.2",
      "debug": "^4.3.4",
      "cross-fetch": "^4.0.0"
    },
    "devDependencies": {
      "@commitlint/cli": "^17.7.1",
      "@commitlint/config-conventional": "^17.7.0",
      "@rollup/plugin-commonjs": "^25.0.4",
      "@rollup/plugin-json": "^6.0.0",
      "@rollup/plugin-node-resolve": "^15.2.1",
      "@rollup/plugin-typescript": "^11.1.3",
      "@types/bn.js": "^5.1.2",
      "@types/debug": "^4.1.9",
      "@types/jest": "^29.5.5",
      "@types/lodash": "^4.14.199",
      "@types/node": "^20.8.2",
      "@types/retry": "^0.12.5",
      "@types/ws": "^8.5.7",
      "@typescript-eslint/eslint-plugin": "^6.7.4",
      "@typescript-eslint/parser": "^6.7.4",
      "eslint": "^8.51.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-import": "^2.28.1",
      "eslint-plugin-jest": "^27.4.2",
      "eslint-plugin-prettier": "^5.0.0",
      "husky": "^8.0.3",
      "jest": "^29.7.0",
      "jest-environment-jsdom": "^29.7.0",
      "lint-staged": "^14.0.1",
      "nock": "^13.3.3",
      "prettier": "^3.0.3",
      "rimraf": "^5.0.5",
      "rollup": "^3.29.4",
      "rollup-plugin-dts": "^6.0.2",
      "rollup-plugin-terser": "^7.0.2",
      "source-map-explorer": "^2.5.3",
      "standard-version": "^9.5.0",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.1",
      "typedoc": "^0.25.2",
      "typescript": "^5.2.2"
    },
    "peerDependencies": {
      "@solana/web3.js": "^1.78.0"
    },
    "commitlint": {
      "extends": [
        "@commitlint/config-conventional"
      ]
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged",
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      }
    },
    "lint-staged": {
      "*.{ts,tsx}": [
        "eslint --fix",
        "prettier --write"
      ]
    },
    "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "collectCoverage": true,
      "collectCoverageFrom": [
        "src/**/*.{ts,tsx}",
        "!src/**/*.d.ts",
        "!src/**/index.ts",
        "!src/**/*.test.{ts,tsx}",
        "!src/**/__tests__/**/*"
      ],
      "coverageThreshold": {
        "global": {
          "branches": 80,
          "functions": 80,
          "lines": 80,
          "statements": 80
        }
      },
      "moduleNameMapper": {
        "^@/(.*)$": "<rootDir>/src/$1"
      }
    },
    "browserslist": [
      "node >= 18",
      "not IE 11",
      "maintained node versions"
    ],
    "resolutions": {
      "minimist": ">=1.2.6",
      "async": ">=2.6.4"
    }
  }